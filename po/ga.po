"      [--limit=N | -l N] [-S | --statistics] [-0 | --null] [-c | --count]\n"
"      [-P | -H | --nofollow] [-L | --follow] [-m | --mmap ] [ -s | --"
"stdio ]\n"
"      [-A | --all] [-p | --print] [-r | --regex ] [--regextype=TYPE]\n"
"      [-version] [--help]\n"
"      pattern...\n"
msgstr ""
"Úsáid: %s [-d conair | --database=conair] [-e | -E | --[non-]existing]\n"
"      [-i | --ignore-case] [-w | --wholename] [-b | --basename] \n"
"      [--limit=N | -l N] [-S | --statistics] [-0 | --null] [-c | --count]\n"
